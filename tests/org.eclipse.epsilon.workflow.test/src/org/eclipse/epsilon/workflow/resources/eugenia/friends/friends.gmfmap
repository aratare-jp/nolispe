<?xml version="1.0" encoding="ASCII"?>
<gmfmap:Mapping xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore"
    xmlns:gmfmap="http://www.eclipse.org/gmf/2008/mappings"
    xmlns:gmftool="http://www.eclipse.org/gmf/2005/ToolDefinition">
  <nodes>
    <containmentFeature
        href="friends.ecore#//World/people"/>
    <ownedChild>
      <domainMetaElement
          href="friends.ecore#//Person"/>
      <labelMappings
          xsi:type="gmfmap:FeatureLabelMapping">
        <diagramLabel
            href="friends.gmfgraph#PersonLabel"/>
        <features
            href="friends.ecore#//Person/name"/>
      </labelMappings>
      <tool
          xsi:type="gmftool:CreationTool"
          href="friends.gmftool#//@palette/@tools.0/@tools.0"/>
      <diagramNode
          href="friends.gmfgraph#Person"/>
    </ownedChild>
  </nodes>
  <links>
    <labelMappings
        xsi:type="gmfmap:DesignLabelMapping"
        readOnly="true">
      <diagramLabel
          href="friends.gmfgraph#PersonFriendOfExternalLabel"/>
    </labelMappings>
    <tool
        xsi:type="gmftool:CreationTool"
        href="friends.gmftool#//@palette/@tools.1/@tools.1"/>
    <diagramLink
        href="friends.gmfgraph#PersonFriendOf"/>
    <linkMetaFeature
        xsi:type="ecore:EReference"
        href="friends.ecore#//Person/friendOf"/>
  </links>
  <links>
    <labelMappings
        xsi:type="gmfmap:DesignLabelMapping"
        readOnly="true">
      <diagramLabel
          href="friends.gmfgraph#PersonEnemyOfExternalLabel"/>
    </labelMappings>
    <tool
        xsi:type="gmftool:CreationTool"
        href="friends.gmftool#//@palette/@tools.1/@tools.0"/>
    <diagramLink
        href="friends.gmfgraph#PersonEnemyOf"/>
    <linkMetaFeature
        xsi:type="ecore:EReference"
        href="friends.ecore#//Person/enemyOf"/>
  </links>
  <diagram>
    <diagramCanvas
        href="friends.gmfgraph#friends"/>
    <domainModel
        href="friends.ecore#/"/>
    <domainMetaElement
        href="friends.ecore#//World"/>
    <palette
        href="friends.gmftool#//@palette"/>
  </diagram>
</gmfmap:Mapping>
